digraph {
	node [shape=square style=filled]
	subgraph cluster_root {
		color=white label=Root
		Start [color=pink shape=circle]
		Start -> 1 [label=Start]
		subgraph cluster_init {
			color=pink label=Init
			1
			Login
			1 -> Login [label=login]
			Login -> 1 [label=incorrect]
		}
		subgraph cluster_lobby {
			color=pink label=Lobby
			2
			Logout
			Login -> 2 [label=ok]
			2 -> Logout [label=logout]
			Logout -> Init [label=completed]
			2 -> Reply [label=game_summary]
		}
		Reply -> Game [label=Agree]
		Reply -> Lobby [label=Reject]
		Game -> Game [label=Move]
		Game -> Game [label=MoveEcho]
		Game -> Init [label=GameoverFloodgate]
		Game -> Lobby [label=GameoverWcsc]
	}
}
